{
    "name": "pfwd/vimeo-console",
    "description": "Symfony console scripts that interact with the Vimeo API",
    "type": "library",
    "keywords": ["vimeo","api", "uri"],
    "homepage": "https://github.com/pfwd/vimeo-console",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Fisher",
            "homepage": "http://peterfisher.me.uk",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/App/"
        }
    },
    "require": {
        "symfony/console": "~4.0",
        "php": ">=7.2",
        "vimeo/vimeo-api": "^3.0",
        "pfwd/vimeo-entities": "^1.0",
        "pfwd/vimeo-api-uri": "^1.0",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.3",
        "codeception/codeception": "^2.5"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-bin-dir": "bin"
    }
}
